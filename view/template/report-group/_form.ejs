<% layout(_view.get(_layout)) %>
<% block('tab', 'Group') %>

<%- include(_view.get('_part/attr/static'), {
    attr: 'report',
    displayValue: model.getRelatedTitle('report'),
    url: ['report/update', model.rel('report')]
}) %>

<%- include(_view.get('_part/group/columns'), {
    items: [{
        template: _view.get('_part/attr/string'),
        params: {
            attr: 'name'
        }
    }, {
        template: _view.get('_part/attr/string'),
        params: {
            attr: 'label'
        }
    }]
}) %>

<%- include(_view.get('_part/group/columns'), {
    items: [{
        template: _view.get('_part/attr/select'),
        params: {
            attr: 'parent',
            items: validParents
        }
    }]
}) %>

<%- include(_view.get('_part/attr/json'), {
    attr: 'options'
}) %>