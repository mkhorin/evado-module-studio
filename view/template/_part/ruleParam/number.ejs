<!-- studio/_part/ruleParam/number -->
<%
const checkboxTemplate = _view.get('_part/attr/checkbox');
const jsonTemplate = _view.get('_part/attr/json');
const stringTemplate = _view.get('_part/attr/string');
%>
<%- include(checkboxTemplate, {
  attr: 'integerOnly'
}) %>

<%- include(stringTemplate, {
  attr: 'min'
}) %>

<%- include(stringTemplate, {
  attr: 'max'
}) %>

<%- include(stringTemplate, {
  attr: 'message'
}) %>

<%- include(jsonTemplate, {
  attr: 'when',
  extHint: model.ATTR_EXT_HINTS.when
}) %>