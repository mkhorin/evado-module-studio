<%
const stringTemplate = _view.get('_part/attr/string');
%>
<% layout(_view.get(_layout)); %>
<% block('tab', 'Class'); %>

<%- include(stringTemplate, {
    attr: 'name'
}) %>

<%- include(stringTemplate, {
    attr: 'label'
}) %>

<% if (model.get('parent')) { %>
<%- include(_view.get('_part/attr/static'), {
    attr: 'parent',
    displayValue: model.getRelatedTitle('parent'),
    url: ['update', model.rel('parent')]
}) %>
<% } %>
